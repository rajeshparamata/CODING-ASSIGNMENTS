#include <stdio.h>

int main() {
   int n, i, highest;
   
   
   printf("Enter the number of inputs: ");
   scanf("%d", &n);

   printf("Enter number 1: ");
   scanf("%d", &highest);

   for (i = 2; i <= n; i++) {
      int num;
      printf("Enter number %d: ", i);
      scanf("%d", &num);
      
      if (num > highest) {
         highest = num;
      }
   }
   
   
   printf("Highest is %d\n", highest);
   return 0;
}
